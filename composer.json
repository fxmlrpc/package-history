{
    "name": "fxmlrpc/sax-serialization",
    "description": "SAX serialization logic for fXmlRpc",
    "license": "MIT",
    "keywords": ["xmlrpc", "xml", "rpc", "serialization", "SAX"],
    "homepage": "http://fxmlrpc.org",
    "authors": [
        {
            "name": "Lars Strojny",
            "email": "lstrojny@php.net",
            "homepage": "http://usrportage.de"
        },
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "fxmlrpc/serialization": "dev-initial_import",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "provide": {
        "fxmlrpc/serialization-implementation": "0.1"
    },
    "autoload": {
        "psr-4": {
            "fXmlRpc\\Serialization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "fXmlRpc\\Serialization\\Tests\\": [
                "tests/",
                "vendor/fxmlrpc/serialization/tests/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
