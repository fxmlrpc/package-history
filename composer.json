{
    "name": "fxmlrpc/native-serialization",
    "description": "Native serialization logic for FXMLRPC",
    "license": "MIT",
    "keywords": ["xmlrpc", "xml", "rpc", "serialization", "native"],
    "homepage": "http://fxmlrpc.org",
    "authors": [
        {
            "name": "Lars Strojny",
            "email": "lstrojny@php.net",
            "homepage": "http://usrportage.de"
        },
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "fxmlrpc/serialization": "^1.0@dev",
        "ext-xmlrpc": "*"
    },
    "require-dev": {
        "fxmlrpc/serialization-integration-tests": "^1.0@dev"
    },
    "provide": {
        "fxmlrpc/serialization-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Fxmlrpc\\Serialization\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
